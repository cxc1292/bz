<?xml version="1.0" encoding="utf-8" ?>
<ThePizzaMaster>
	<Page name="MainPage">
		<control name="Label1" Setter="Text">
			<lang name="en">Hello, Pizza Lovers!</lang>
			<lang name="zh-Hans">您好，披萨发烧友！</lang>
			<lang name="ja">ピザ愛好家の皆さん、こんにちは！</lang>
			<lang name="fr">Bonjour, amateurs de pizza !</lang>
			<lang name="ar">مرحبًا، عشاق البيتزا!</lang>
			<lang name="es">¡Hola, amantes de la pizza!</lang>
			<lang name="ko">안녕하세요, 피자 애호가 여러분!</lang>
			<lang name="zh-Hant">您好，披薩發燒友！</lang>
			<lang name="hi">नमस्ते, पिज़्ज़ा प्रेमिकों!</lang>
			<lang name="de">Hallo, Pizza-Liebhaber!</lang>
		</control>
		<control name="Label2" Setter="Text">
			<lang name="en">Let's start with choosing a dough recipe.</lang>
			<lang name="zh-Hans">请您选择一款菜单。</lang>
			<lang name="ja">生地のレシピを選びましょう。</lang>
			<lang name="fr">Commençons par choisir une recette de pâte.</lang>
			<lang name="ar">لنبدأ باختيار وصفة العجينة.</lang>
			<lang name="es">Empecemos eligiendo una receta de masa.</lang>
			<lang name="ko">반죽 레시피를 선택해 보겠습니다.</lang>
			<lang name="zh-Hant">讓我們從選擇麵團食譜開始。</lang>
			<lang name="hi">आइए डो रेसिपी का चयन करें।</lang>
			<lang name="de">Lassen Sie uns mit der Auswahl eines Teigrezepts beginnen.</lang>
		</control>
	</Page>
	<Page name="StagingPage">
		<control name="Label1" Setter="Text">
			<lang name="English">Selected Recipe</lang>
			<lang name="zh-Hans">选定的食谱</lang>
			<lang name="ja">選択したレシピ</lang>
			<lang name="fr">Recette sélectionnée</lang>
			<lang name="ar">الوصفة المحددة</lang>
			<lang name="es">Receta seleccionada</lang>
			<lang name="ko">선택한 레시피</lang>
			<lang name="zh-Hant">選定的食譜</lang>
			<lang name="hi">चयनित रेसिपी</lang>
			<lang name="de">Ausgewähltes Rezept</lang>
		</control>
		<control name="Label2" Setter="Text">
			<lang name="English">Calculate by</lang>
			<lang name="zh-Hans">计算方法</lang>
			<lang name="ja">計算方法</lang>
			<lang name="fr">Calculer par</lang>
			<lang name="ar">حساب بواسطة</lang>
			<lang name="es">Calcular por</lang>
			<lang name="ko">계산 방법</lang>
			<lang name="zh-Hant">計算方法</lang>
			<lang name="hi">के द्वारा गणना करें</lang>
			<lang name="de">Berechnen nach</lang>
		</control>
		<control name="Label3" Setter="Text">
			<lang name="English">Pizza size</lang>
			<lang name="zh-Hans">比萨尺寸</lang>
			<lang name="ja">ピザのサイズ</lang>
			<lang name="fr">Taille de la pizza</lang>
			<lang name="ar">حجم البيتزا</lang>
			<lang name="es">Tamaño de la pizza</lang>
			<lang name="ko">피자 크기</lang>
			<lang name="zh-Hant">比薩尺寸</lang>
			<lang name="hi">पिज़्ज़ा का आकार</lang>
			<lang name="de">Pizzagröße</lang>
		</control>
		<control name="PizzaSize" Setter="Text">
			<lang name="English">Pizza size</lang>
			<lang name="zh-Hans">比萨尺寸</lang>
			<lang name="ja">ピザのサイズ</lang>
			<lang name="fr">Taille de la pizza</lang>
			<lang name="ar">حجم البيتزا</lang>
			<lang name="es">Tamaño de la pizza</lang>
			<lang name="ko">피자 크기</lang>
			<lang name="zh-Hant">比薩尺寸</lang>
			<lang name="hi">पिज़्ज़ा का आकार</lang>
			<lang name="de">Pizzagröße</lang>
		</control>
		<control name="NumberOfPizzas">
			<lang name="English">Number of Pizzas</lang>
			<lang name="zh-Hans">比萨数量</lang>
			<lang name="ja">ピザの数</lang>
			<lang name="fr">Nombre de pizzas</lang>
			<lang name="ar">عدد البيتزا</lang>
			<lang name="es">Número de pizzas</lang>
			<lang name="ko">피자 수</lang>
			<lang name="zh-Hant">比薩數量</lang>
			<lang name="hi">पिज़्ज़ा की संख्या</lang>
			<lang name="de">Anzahl der Pizzen</lang>

		</control>
		<control name="TotalWeight" Setter="Text">
			<lang name="English">Total Weight</lang>
			<lang name="zh-Hans">总重量</lang>
			<lang name="ja">総重量</lang>
			<lang name="fr">Poids total</lang>
			<lang name="ar">الوزن الكلي</lang>
			<lang name="es">Peso total</lang>
			<lang name="ko">총 무게</lang>
			<lang name="zh-Hant">總重量</lang>
			<lang name="hi">कुल वजन</lang>
			<lang name="de">Gesamtgewicht</lang>
		</control>
	</Page>
</ThePizzaMaster>
